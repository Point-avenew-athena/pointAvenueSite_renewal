
<li class="class_box_type_01--item" data-id="<%-course?.courseId-%>">
  <div class="class_box_type_01--img">
    <img src="<%-course?.thumbnail-%>" onError="this.src='/images/common/none_img.png'" alt="<%-course?.name-%> thumbnail">
  </div>
  <div class="class_box_type_01--desc">
    <div class="text--gray">
      <p class="age">Age <%-course?.age-%></p>
      <p class="times"><%-course?.runtime-%></p>
    </div>
    <p class="title_r_03"><%-course?.name-%></p>
    <span class="class_type"><%-course?.courseType-%></span>
  </div>
</li>