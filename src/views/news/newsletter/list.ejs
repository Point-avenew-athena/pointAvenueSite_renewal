<%- include("../../partials/head_start") %>
<title>Newsletters - Point Avenue</title>
<meta name="keywords" content="Newsletters, Point Avenue">
<meta name="title" content="Newsletters - Point Avenue">
<meta property="og:title" content="Newsletters - Point Avenue">


<link rel="stylesheet" type="text/css" href="/css/reset.css">
<link rel="stylesheet" type="text/css" href="/css/swiper.min.css">
<link rel="stylesheet" type="text/css" href="/css/common.css">
<link rel="stylesheet" type="text/css" href="/css/layout.css">
<link rel="stylesheet" type="text/css" href="/css/sub.css">

<script language="JavaScript" type="text/javascript" src="/js/jquery-3.6.0.min.js"></script>
<script language="JavaScript" type="text/javascript" src="/js/swiper.min.js"></script>
<script language="JavaScript" type="text/javascript" src="/js/common.js"></script>
<script language="JavaScript" type="text/javascript" src="/js/sub/admission-consulting/admission-consulting.js"></script>

<script>
	const newsletters = <%- JSON.stringify(newsletters)%>;
</script>
<%- include("../../partials/head_end") %>
<%- include("../../partials/body_start") %>

<div class="wrap sub news newsletter_list">
  <%- include("../../partials/gnb") %>
  <!-- container :: start -->
		<div class="container">
			<section class="visual_wrap">
				<div class="section_inner">
					<div class="text_box">
						<h2 class="title_r_01">Newsletters</h2>
					</div>
				</div>
			</section>
			
			<section class="newsletter_wrap">
				<div class="section_inner">
					<h3 class="title_r_02 section_title">Newsletters</h3>
					<ul class="box_type_01 courses_list">
						<% 
							const onlyNewsletters = newsletters.filter(newsletter => (newsletter.type == "Newsletter"));
						%>
						<% for ( let i = 0; i < onlyNewsletters.length; i++){ %>
							<li class="box_type_01--item courses_item">
								<div class="box_type_01--img courses_img">
									<a href="/news/<%= onlyNewsletters[i].id %>">
										<img src="<%= onlyNewsletters[i].thumbnail.imageUrl %>" alt="<%= onlyNewsletters[i].title %>">
									</a>
								</div>
								<div class="box_type_01--desc courses_desc_box">
									<a href="/news/<%= onlyNewsletters[i].id %>">
										<p class="title_r_03"><%= onlyNewsletters[i].title %></p>
									</a>
									<div class="desc_text">
										<a href="/news/<%= onlyNewsletters[i].id %>">
											<%- onlyNewsletters[i].thumbnail.excerpt %>
										</a>
									</div>
									<a href="/news/<%= onlyNewsletters[i].id %>" class="btn_type_01--sec">
										<span class="btn_inner">Read More</span>
									</a>
								</div>
							</li>
						<% } %>
					</ul>
				</div>
			</section>
			
			
		</div>
		<!--container :: end-->
    <%- include("../../partials/footer") %>
		<%- include("../../partials/common_popup") %>
</div>
<%- include("../../partials/body_end") %>