	const teamMembers = <%- JSON.stringify(teamMembers) %>;
	const courses = <%- JSON.stringify(courses) %>;
	$(function(){
		const type = 'middlehigh';
		const limit = 0;
		renderCourse(type, limit);
		
		// 필터 클릭시
		$('.option_list .option_item .filter_item input[type="checkbox"]').change(function () {
			const on = this.checked;
			const value = this.value;
			const option = this.dataset.option;
			courseFilter(option, value, on);
			renderCourse(type, limit);
		});

		// sort
		$('.sort_by_box .sort_kind_item a').click(function() {
			courseSort = this.dataset.value;
		});
	});
